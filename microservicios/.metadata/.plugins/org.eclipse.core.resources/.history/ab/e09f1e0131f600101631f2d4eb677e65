spring:
  application:
    name:
      api-gateway
  cloud:
    loadbalancer:
      enabled: true

    gateway:
      server:
        webflux:
          routes:
            - id: msv-pacientes2
              uri: lb://msv-pacientes2
              predicates: 
                - Path=/api/pacientes/**
              filters:
                -StripPrefix=2

server:
  port: 8090

eureka:
  instance:
    instance-id: ${spring.application.name}:${random.value}

  client:
    service-url:
      defaultZone: http://localhost:8761/eureka
